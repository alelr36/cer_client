{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "settings": {
    "import/resolver": "webpack"
  },
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unexpected-multiline": 2,
    "class-methods-use-this": 0,
    "semi": [2, "never"],
    "complexity": [2, 2],
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "react/jsx-uses-vars": [2],
    "react/jsx-uses-react": 1
  },
  "globals": {
    "_": true
  }
}